<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object id="main_frame" class="tk.Frame">
    <property name="height">200</property>
    <property name="width">200</property>
    <layout>
      <property name="column">0</property>
      <property name="propagate">True</property>
      <property name="row">0</property>
    </layout>
    <child>
      <object id="tasks" class="tk.LabelFrame">
        <property name="height">200</property>
        <property name="text" translatable="yes">Tasks</property>
        <property name="width">200</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">nw</property>
        </layout>
        <child>
          <object id="open_generic_chests" class="ttk.Checkbutton">
            <property name="text" translatable="yes">Open Generic Chests</property>
            <layout>
              <property name="column">0</property>
              <property name="columnspan">3</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object id="open_masterwork_chests" class="ttk.Checkbutton">
            <property name="text" translatable="yes">Open Masterwork Chests</property>
            <layout>
